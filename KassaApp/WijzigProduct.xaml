<Window x:Class="KassaApp.WijzigProduct"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KassaApp"
        mc:Ignorable="d"
        Title="WijzigProduct" Height="400" Width="400">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Pictures/Background.png" Stretch="UniformToFill"/>
        </Grid.Background>
        <DataGrid x:Name="DGProductenSelector" HorizontalAlignment="Left" Height="369" VerticalAlignment="Top" Width="392" MouseDoubleClick="DGProductenSelector_MouseDoubleClick" Background="{x:Null}"/>

    </Grid>
</Window>
